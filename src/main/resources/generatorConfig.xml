<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--去掉注释-->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--需要配置数据库连接-->
        <jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
                        connectionURL="jdbc:oracle:thin:@10.82.82.166:1521:orcl"
                        userId="YKGC"
                        password="zyk27ZYK">
        </jdbcConnection>
        <javaTypeResolver type="com.prtr.ykgc.OracleJavaTypeResolver">
            <!--<property  name="forceBigDecimals" value="false"/>-->
        </javaTypeResolver>

        <!--指定entity生成的位置-->
        <javaModelGenerator targetPackage="com.prtr.ykgc.entity" targetProject="./src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!--sql映射文件生成的位置 mapper.xml-->
        <sqlMapGenerator targetPackage="mapper" targetProject="./src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!--指定DaoMapper生成的位置 mapper.java-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.prtr.ykgc.mapper"
                             targetProject="./src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--table是指定每个表的生成策略-->


        <table tableName="TB_ROLE" domainObjectName="Role">
            <columnOverride column="ROLEID" property="roleId"></columnOverride>
            <columnOverride column="ROLENAME" property="roleName"></columnOverride>
            <columnOverride column="CREATETIME" property="createTime"></columnOverride>
            <columnOverride column="TYPE" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="ROLELEVEL" javaType="java.lang.Integer" property="roleLevel"></columnOverride>
        </table>
        <table tableName="TB_ROLE_MENU" domainObjectName="RoleMenu">
            <columnOverride column="MENUID" javaType="java.lang.Integer" property="menuId"></columnOverride>
            <columnOverride column="MENUNAME" property="menuName"></columnOverride>
            <columnOverride column="MENUFATHERID" javaType="java.lang.Integer" property="menuFatherId"></columnOverride>
            <columnOverride column="SORT" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="ISINNER" javaType="java.lang.Integer" property="isInner"></columnOverride>
            <columnOverride column="ISOUTER" javaType="java.lang.Integer" property="isOuter"></columnOverride>
            <columnOverride column="STATUS" javaType="java.lang.Integer"></columnOverride>
        </table>
        <table tableName="TB_ROLE_MENUITEM" domainObjectName="RoleMenuItem">
            <columnOverride column="MENUINNERID" javaType="java.lang.Integer" property="menuInnerId"></columnOverride>
            <columnOverride column="ROLEID" property="roleId"></columnOverride>
            <columnOverride column="ROLEMENUID" javaType="java.lang.Integer" property="roleMenuId"></columnOverride>
        </table>

        <table tableName="TB_USER_ROLES" domainObjectName="UserRoles">
            <columnOverride column="USERID" property="userId"></columnOverride>
            <columnOverride column="ROLEID" property="roleId"></columnOverride>
        </table>
        <table tableName="TB_USER_SESSION" domainObjectName="UserSession">
            <columnOverride column="SESSIONID" property="sessionId"></columnOverride>
            <columnOverride column="LASTLOGINTIME" property="lastLoginTime"></columnOverride>
            <columnOverride column="USERTYPE" javaType="java.lang.Integer" property="userType"></columnOverride>
            <columnOverride column="LOGINIP" property="loginIp"></columnOverride>
            <columnOverride column="LOGFROM" javaType="java.lang.Integer" property="logFrom"></columnOverride>
        </table>


        <table tableName="TB_USERS" domainObjectName="User">
            <columnOverride column="USERID" property="userId"></columnOverride>
            <columnOverride column="USERPASSWORD" property="userPassword"></columnOverride>
            <columnOverride column="REGISTERSITEID" property="registerSiteId"></columnOverride>
            <columnOverride column="REALNAME" property="realName"></columnOverride>
            <columnOverride column="CARDID" property="carId"></columnOverride>
            <columnOverride column="DEPARTMENTID" property="departmentId"></columnOverride>
            <columnOverride column="LASTLOGINTIME" property="lastLoginTime"></columnOverride>
            <columnOverride column="ROLEID" property="roleId"></columnOverride>
            <columnOverride column="POLICENUMBER" property="policeNumber"></columnOverride>
            <columnOverride column="OLDERID" property="oldErId"></columnOverride>
            <columnOverride column="DEPTGRADE" javaType="java.lang.Integer" property="deptGrade"></columnOverride>
            <columnOverride column="CREATETIME" property="createTime"></columnOverride>
            <columnOverride column="CANDEL" javaType="java.lang.Integer" property="canDel"></columnOverride>
            <columnOverride column="ISDEL" javaType="java.lang.Integer" property="isDel"></columnOverride>
            <columnOverride column="ISADMIN" javaType="java.lang.Integer" property="isAdmin"></columnOverride>
        </table>
        <table tableName="TB_POLICE" domainObjectName="Police">
            <columnOverride column="POLICE_LEVEL" javaType="java.lang.Integer"></columnOverride>
        </table>

    </context>
</generatorConfiguration>
